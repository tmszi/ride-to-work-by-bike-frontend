    "eslint-plugin-cypress": "^3.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19 || ^20.0.0",
